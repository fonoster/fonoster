{
  "name": "@fonos/ctl",
  "version": "0.0.10",
  "description": "Command-Line for Fonos",
  "author": "Pedro Sanders <sanderspedro@gmail.com>",
  "homepage": "https://github.com/fonoster/fonos#readme",
  "license": "MIT",
  "main": "dist/index.js",
  "types": "dist/index.js",
  "bin": {
    "fonos": "./bin/run"
  },
  "engines": {
    "node": ">=8.0.0"
  },
  "scripts": {
    "build": "npm run clean && npm run compile",
    "clean": "rm -rf ./dist && rm -rf tsconfig.tsbuildinfo",
    "compile": "tsc -b tsconfig.json",
    "postpack": "rm -f oclif.manifest.json",
    "prepack": "oclif-dev manifest && oclif-dev readme",
    "test": "nyc mocha --forbid-only \"test/**/*.test.js\"",
    "version": "oclif-dev readme && git add README.md"
  },
  "directories": {
    "src": "src",
    "test": "test",
    "bin": "bin"
  },
  "files": [
    "/bin",
    "/npm-shrinkwrap.json",
    "/oclif.manifest.json",
    "/dist"
  ],
  "oclif": {
    "commands": "./dist/commands",
    "bin": "fonos",
    "plugins": [
      "@oclif/plugin-help"
    ],
    "topics": {
      "apps": {
        "description": "manage voice applications"
      },
      "domains": {
        "description": "manage sip domains"
      },
      "agents": {
        "description": "manage sip agents"
      },
      "numbers": {
        "description": "manage sip numbers"
      },
      "providers": {
        "description": "manage sip providers"
      },
      "login": {
        "description": "authenticates current station"
      },
      "logout": {
        "description": "revoke crendentials to the current station"
      }
    }
  },
  "publishConfig": {
    "access": "public"
  },
  "repository": {
    "type": "git",
    "url": "git+https://github.com/fonoster/fonos.git"
  },
  "bugs": {
    "url": "https://github.com/fonoster/fonos/issues"
  },
  "dependencies": {
    "@fonos/agents": "^0.0.10",
    "@fonos/appmanager": "^0.0.10",
    "@fonos/core": "^0.0.10",
    "@fonos/domains": "^0.0.10",
    "@fonos/logger": "^0.0.10",
    "@fonos/numbers": "^0.0.10",
    "@fonos/providers": "^0.0.10",
    "@oclif/command": "^1.5.19",
    "@oclif/config": "^1.14.0",
    "@oclif/plugin-help": "^2.2.3",
    "cli-ux": "^5.4.5",
    "easy-table": "^1.1.1",
    "inquirer": "^7.1.0",
    "moment": "^2.24.0",
    "node-plop": "^0.25.0",
    "ora": "^4.0.3",
    "prettyjson": "^1.2.1",
    "truncate": "^2.1.0",
    "tslib": "^1"
  },
  "devDependencies": {
    "@types/prettyjson": "0.0.29",
    "dotenv": "^8.2.0",
    "typescript": "^3.9.3"
  }
}
