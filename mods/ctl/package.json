{
  "name": "@fonos/ctl",
  "version": "0.0.120",
  "description": "Command-Line for Fonos",
  "author": "Pedro Sanders <psanders@fonoster.com>",
  "homepage": "https://github.com/fonoster/fonos#readme",
  "license": "MIT",
  "main": "dist/index.js",
  "types": "dist/index.js",
  "bin": {
    "fonos": "./bin/run"
  },
  "engines": {
    "node": ">=8.0.0"
  },
  "scripts": {
    "prebuild": "rimraf ./dist tsconfig.tsbuildinfo",
    "build": "tsc -b tsconfig.json",
    "postpack": "rimraf -f oclif.manifest.json",
    "prepack": "oclif-dev manifest && oclif-dev readme",
    "version": "oclif-dev readme && git add README.md"
  },
  "directories": {
    "src": "src",
    "test": "test",
    "bin": "bin"
  },
  "files": [
    "/bin",
    "/npm-shrinkwrap.json",
    "/oclif.manifest.json",
    "/dist"
  ],
  "oclif": {
    "commands": "./dist/commands",
    "bin": "fonos",
    "plugins": [
      "@oclif/plugin-help",
      "@oclif/plugin-plugins",
      "@oclif/plugin-not-found",
      "@oclif/plugin-warn-if-update-available"
    ],
    "warn-if-update-available": {
      "timeoutInDays": 1,
      "message": "<%= config.name %> update available from <%= chalk.greenBright(config.version) %> to <%= chalk.greenBright(latest) %>."
    },
    "topics": {
      "domains": {
        "description": "manage sip domains"
      },
      "agents": {
        "description": "manage sip agents"
      },
      "numbers": {
        "description": "manage sip numbers"
      },
      "providers": {
        "description": "manage sip providers"
      },
      "auth": {
        "description": "manage fonos credentials"
      }
    }
  },
  "publishConfig": {
    "access": "public"
  },
  "repository": {
    "type": "git",
    "url": "git+https://github.com/fonoster/fonos.git"
  },
  "bugs": {
    "url": "https://github.com/fonoster/fonos/issues"
  },
  "dependencies": {
    "@fonos/agents": "^0.0.120",
    "@fonos/domains": "^0.0.120",
    "@fonos/logger": "^0.0.120",
    "@fonos/numbers": "^0.0.120",
    "@fonos/providers": "^0.0.120",
    "@oclif/command": "^1.8.0",
    "@oclif/config": "^1.17.0",
    "@oclif/errors": "^1.3.4",
    "@oclif/plugin-help": "^3.2.2",
    "@oclif/plugin-not-found": "1.2.4",
    "@oclif/plugin-plugins": "^1.10.0",
    "@oclif/plugin-warn-if-update-available": "^1.7.0",
    "cli-ux": "^5.5.1",
    "dockerode": "^3.3.0",
    "easy-table": "^1.1.1",
    "inquirer": "^7.3.3",
    "moment": "^2.29.1",
    "phone": "^2.4.21",
    "prettyjson": "^1.2.1",
    "promise-fs": "^2.1.1",
    "truncate": "^2.1.0",
    "update-notifier": "^5.1.0"
  },
  "devDependencies": {
    "@types/dockerode": "^3.2.2",
    "@types/phone": "^2.4.0",
    "@types/prettyjson": "0.0.29",
    "rimraf": "^3.0.2"
  },
  "gitHead": "e7d1d6d4b29f2e0b4b37a55ea4026931a3f76d1b"
}
