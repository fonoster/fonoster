{
  "name": "@fonos/ctl",
  "version": "0.0.80",
  "description": "Command-Line for Fonos",
  "author": "Pedro Sanders <sanderspedro@gmail.com>",
  "homepage": "https://github.com/fonoster/fonos#readme",
  "license": "MIT",
  "main": "dist/index.js",
  "types": "dist/index.js",
  "bin": {
    "fonos": "./bin/run"
  },
  "engines": {
    "node": ">=8.0.0"
  },
  "scripts": {
    "prebuild": "rimraf ./dist tsconfig.tsbuildinfo",
    "postbuild": "cp -a ./src/templates ./dist",
    "build": "tsc -b tsconfig.json",
    "postpack": "rm -f oclif.manifest.json",
    "prepack": "oclif-dev manifest && oclif-dev readme",
    "version": "oclif-dev readme && git add README.md"
  },
  "directories": {
    "src": "src",
    "test": "test",
    "bin": "bin"
  },
  "files": [
    "/bin",
    "/npm-shrinkwrap.json",
    "/oclif.manifest.json",
    "/dist"
  ],
  "oclif": {
    "commands": "./dist/commands",
    "bin": "fonos",
    "plugins": [
      "@oclif/plugin-help"
    ],
    "topics": {
      "apps": {
        "description": "manage voice applications"
      },
      "domains": {
        "description": "manage sip domains"
      },
      "agents": {
        "description": "manage sip agents"
      },
      "numbers": {
        "description": "manage sip numbers"
      },
      "providers": {
        "description": "manage sip providers"
      },
      "auth": {
        "description": "manage fonos credentials"
      }
    }
  },
  "publishConfig": {
    "access": "public"
  },
  "repository": {
    "type": "git",
    "url": "git+https://github.com/fonoster/fonos.git"
  },
  "bugs": {
    "url": "https://github.com/fonoster/fonos/issues"
  },
  "dependencies": {
    "@fonos/agents": "^0.0.80",
    "@fonos/appmanager": "^0.0.80",
    "@fonos/core": "^0.0.80",
    "@fonos/domains": "^0.0.80",
    "@fonos/logger": "^0.0.80",
    "@fonos/numbers": "^0.0.80",
    "@fonos/providers": "^0.0.80",
    "@kubernetes/client-node": "^0.13.2",
    "@oclif/command": "^1.8.0",
    "@oclif/config": "^1.17.0",
    "@oclif/plugin-help": "^3.2.2",
    "btoa": "^1.2.1",
    "cli-ux": "^5.5.1",
    "dockerode": "^3.2.1",
    "easy-table": "^1.1.1",
    "inquirer": "^7.3.3",
    "moment": "^2.29.1",
    "node-plop": "^0.26.2",
    "ora": "^5.3.0",
    "phone": "^2.4.20",
    "prettyjson": "^1.2.1",
    "promise-fs": "^2.1.1",
    "truncate": "^2.1.0",
    "ts-node": "^9.1.1",
    "tslib": "^2",
    "update-notifier": "^5.1.0"
  },
  "devDependencies": {
    "@types/dockerode": "^3.2.2",
    "@types/phone": "^2.4.0",
    "@types/prettyjson": "0.0.29",
    "@types/update-notifier": "^5.0.0",
    "dotenv": "^8.2.0",
    "rimraf": "^3.0.2",
    "typescript": "^4.1.5"
  },
  "gitHead": "8efbe2e8601b3a0618e770804a205e8584d49580"
}
